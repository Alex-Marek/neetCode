def isValid(s: str) -> bool:
    par_Count = {"(" : 0, "[" : 0, "{" : 0 }
    for value in s:
        if value == "(" or value == ")":
            if value == "(" : par_Count[value] += 1
            if value == ")" : par_Count["("] -= 1
        
        if value == "[" or value == "]":
            if value == "[" : par_Count[value] += 1
            if value == "]" : par_Count["["] -= 1
        
        if value == "{" or value == "}":
            if value == "{" : par_Count[value] += 1
            if value == "}" : par_Count["{"] -= 1
    for value in par_Count.values():
        if value > 0 or value < 0:
            return False
    return True

print(isValid("()"), "Expected True")
print(isValid("()[]{}"), "Expected True")
print(isValid("(]"), "Expected False")

